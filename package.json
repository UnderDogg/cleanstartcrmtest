{
    "title": "SB Admin",
    "name": "startbootstrap-sb-admin",
    "version": "5.0.2",
    "description": "A one page app landing page HTML theme for Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template",
        "admin",
        "app"
    ],
    "homepage": "https://startbootstrap.com/template-overviews/sb-admin",
    "bugs": {
        "url": "https://github.com/BlackrockDigital/startbootstrap-sb-admin/issues",
        "email": "feedback@startbootstrap.com"
    },
    "license": "MIT",
    "author": "Start Bootstrap",
    "contributors": [
        "David Miller (http://davidmiller.io/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/BlackrockDigital/startbootstrap-sb-admin.git"
    },
    "dependencies": {
        "@coreui/coreui": "^2.0.14",
        "@fortawesome/fontawesome-free": "^5.3.1",
        "bootstrap": "^4.1.3",
        "chart.js": "2.7.2",
        "datatables.net-bs4": "1.10.19",
        "font-awesome": "^4.7.0",
        "jquery": "^3.3.1",
        "jquery.easing": "^1.4.1",
        "popper.js": "^1.14.4"
    },
    "devDependencies": {
        "browser-sync": "2.24.7",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "6.0.0",
        "gulp-clean-css": "3.10.0",
        "gulp-header": "2.0.5",
        "gulp-rename": "1.4.0",
        "gulp-sass": "4.0.1",
        "gulp-uglify": "3.0.1"
    },
    "scripts": {
        "copy-bootstrap": "cp node_modules/bootstrap/dist/js/bootstrap.js resources/assets/js/",
        "copy-coreui-css": "cp node_modules/@coreui/coreui/dist/css/coreui.css resources/assets/css/",
        "copy-coreui-js": "cp node_modules/@coreui/coreui/dist/js/coreui.js resources/assets/js/",
        "copy-coreui-utilities": "cp node_modules/@coreui/coreui/dist/js/coreui-utilities.js resources/assets/js/",
        "copy-fontawesome-css": "cp node_modules/@fortawesome/fontawesome-free/css/all.css resources/assets/fontawesome/css/fontawesome.css",
        "copy-fontawesome-fonts": "cp -r node_modules/@fortawesome/fontawesome-free/webfonts/ resources/assets/fontawesome/",
        "copy-jquery": "cp node_modules/jquery/dist/jquery.js resources/assets/js/",
        "copy-perfect-scrollbar-js": "cp node_modules/perfect-scrollbar/dist/perfect-scrollbar.js resources/assets/js/",
        "copy-popper": "cp node_modules/popper.js/dist/umd/popper.js resources/assets/js/",
        "copy-assets": "npm run copy-bootstrap && npm run copy-coreui-css && npm run copy-coreui-js && npm run copy-coreui-utilities && npm run copy-fontawesome-css && npm run copy-fontawesome-fonts && npm run copy-jquery && npm run copy-perfect-scrollbar-js && npm run copy-popper"
    }
}
